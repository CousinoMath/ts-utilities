{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {},
  "rules": {
    "no-console": [true, "warning"],
    "no-empty": [true, "warning"],
    "no-null-keyword": true,
    "quotemark": [true, "single"],
    "radix": true,
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  },
  "rulesDirectory": []
}
